Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #fff;
}

#dynamic {
  marker-fill:teal;
  marker-line-color:darken(teal,10%);
  marker-width:[size];
  marker-line-width:2;
  marker-opacity:.5;
  marker-allow-overlap:true;
}



