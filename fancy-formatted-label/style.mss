Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #fff;
}

#cowlitz {
  marker-width:10;
  text-face-name:'DejaVu Sans Book';
  text-size:15;
  text-name:"<Format fill='blue' face-name='DejaVu Sans Oblique'>[name]</Format>+'\n'+[ele]+'m'";
  text-dx:10;
}